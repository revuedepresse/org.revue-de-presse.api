{
    "name": "potichats/worker",
    "license": "GPL-3.0-or-later",
    "description": "Automated worker curating tweets by using available HTTP API (application programming API).",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "abraham/twitteroauth": "^4.0",
        "beberlei/assert": "^3.3",
        "beberlei/doctrineextensions": "^1.3.0",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/inflector": "^2.0",
        "doctrine/orm": "^2.8",
        "guzzlehttp/guzzle": "^7.4",
        "kreait/firebase-bundle": "^4.0",
        "ocramius/package-versions": "^1.9",
        "ocramius/proxy-manager": "^2.14",
        "oro/doctrine-extensions": "^2.0",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.8",
        "scienta/doctrine-json-functions": "^5.3",
        "symfony/console": "^5.4",
        "symfony/doctrine-messenger": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/messenger": "^5.4",
        "symfony/mime": "^5.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/string": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/translation-contracts": "^2.3",
        "symfony/uid": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/var-exporter": "^5.4",
        "symfony/yaml": "^5.4",
        "thecodingmachine/safe": "^2.2.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.1.6"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-tests-dir": "tests"
    }
}
